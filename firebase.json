{
  "apphosting": {
    "backendId": "ai-game-discovery-engine",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  },
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    }
  },
  "functions": {
    "source": "src/ai",
    "runtime": "nodejs20"
  }
}